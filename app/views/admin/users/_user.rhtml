<tr class="<%= cycle("even", "odd") -%>">
  <td><%= (@users.index(user).to_i+1).to_s %>.</td>
  <td><%= user.login %></td>
  <td><%= user.email %></td>
  <td><%= user.invitations %></td>
  
  <td>
    <%= link_to set_button("edit"), edit_admin_user_path(user) %>
    <%= link_to set_button("destroy"), admin_user_path(user), :method => :delete, :confirm => "Are you sure?" %>
  </td>
</tr>