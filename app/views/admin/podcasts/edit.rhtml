<% content_for :tc_left do %>
	<h1>Podcasts</h1>
<% end %>
<% content_for :tc_right do%>
	<h1>Edit Podcast</h1>
<% end %>
<%=render :partial => "includes/admin/top_control"%>

<div class="span-24">
<% form_for :podcast, :url => admin_podcast_path(@podcast.id), :html => {:multipart => true, :method => :put}, :builder => TemplatedFormBuilder do |f| %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <%= submit_tag "PUSH" %>
<% end %>
</div>